        <div class="modal-header">
          <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
          <h4 class="modal-title">Transaction {{ transaction.id }}</h4>
        </div>
        <div class="modal-body">
            <div class="printable">
                <h4>Date</h4>
                {{ transaction.transactiondate|date('d F Y') }}
                <h4>Transaction Method</h4>
                {{ transaction.transactionmethod }}
                <h4>Amount</h4>
                {{ transaction.amount }}
                <h4>Customer</h4>
                {{ transaction.firstname }} {{transaction.name }}
                <h4>Note</h4>
                {{ transaction.note }}
            
            </div>
            <div class="clearfix"></div>
            <hr/>
            <p>
               {# <button type="button" class="btn btn-primary" onclick="printPayment();">Print</button>#}
                <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
            </p>
        </div>


            
