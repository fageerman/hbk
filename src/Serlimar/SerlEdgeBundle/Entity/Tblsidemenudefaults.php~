     * @return integer
     */
    public function getFooterhighlightcolor()
    {
        return $this->footerhighlightcolor;
    }

    /**
     * Set footerhighlightbold
     *
     * @param boolean $footerhighlightbold
     *
     * @return Tblsidemenudefaults
     */
    public function setFooterhighlightbold($footerhighlightbold)
    {
        $this->footerhighlightbold = $footerhighlightbold;

        return $this;
    }

    /**
     * Get footerhighlightbold
     *
     * @return boolean
     */
    public function getFooterhighlightbold()
    {
        return $this->footerhighlightbold;
    }

    /**
     * Set footerhighlightunderline
     *
     * @param boolean $footerhighlightunderline
     *
     * @return Tblsidemenudefaults
     */
    public function setFooterhighlightunderline($footerhighlightunderline)
    {
        $this->footerhighlightunderline = $footerhighlightunderline;

        return $this;
    }

    /**
     * Get footerhighlightunderline
     *
     * @return boolean
     */
    public function getFooterhighlightunderline()
    {
        return $this->footerhighlightunderline;
    }
}

