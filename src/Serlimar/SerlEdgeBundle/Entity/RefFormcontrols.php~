<?php

namespace Serlimar\SerlEdgeBundle\Entity;

/**
 * RefFormcontrols
 */
class RefFormcontrols
{
    /**
     * @var integer
     */
    private $formcontrolsid;

    /**
     * @var string
     */
    private $formcontrol;

    /**
     * @var string
     */
    private $description;

    /**
     * @var string
     */
    private $location;

    /**
     * @var string
     */
    private $option;

    /**
     * @var string
     */
    private $type;

    /**
     * @var integer
     */
    private $goupno;

    /**
     * @var integer
     */
    private $sortno;


    /**
     * Get formcontrolsid
     *
     * @return integer
     */
    public function getFormcontrolsid()
    {
        return $this->formcontrolsid;
    }

    /**
     * Set formcontrol
     *
     * @param string $formcontrol
     *
     * @return RefFormcontrols
     */
    public function setFormcontrol($formcontrol)
    {
        $this->formcontrol = $formcontrol;

        return $this;
    }

    /**
     * Get formcontrol
     *
     * @return string
     */
    public function getFormcontrol()
    {
        return $this->formcontrol;
    }

    /**
     * Set description
     *
     * @param string $description
     *
     * @return RefFormcontrols
     */
    public function setDescription($description)
    {
        $this->description = $description;

        return $this;
    }

    /**
     * Get description
     *
     * @return string
     */
    public function getDescription()
    {
        return $this->description;
    }

    /**
     * Set location
     *
     * @param string $location
     *
     * @return RefFormcontrols
     */
    public function setLocation($location)
    {
        $this->location = $location;

        return $this;
    }

    /**
     * Get location
     *
     * @return string
     */
    public function getLocation()
    {
        return $this->location;
    }

    /**
     * Set option
     *
     * @param string $option
     *
     * @return RefFormcontrols
     */
    public function setOption($option)
    {
        $this->option = $option;

        return $this;
    }

    /**
     * Get option
     *
     * @return string
     */
    public function getOption()
    {
        return $this->option;
    }

    /**
     * Set type
     *
     * @param string $type
     *
     * @return RefFormcontrols
     */
    public function setType($type)
    {
        $this->type = $type;

        return $this;
    }

    /**
     * Get type
     *
     * @return string
     */
    public function getType()
    {
        return $this->type;
    }

    /**
     * Set goupno
     *
     * @param integer $goupno
     *
     * @return RefFormcontrols
     */
    public function setGoupno($goupno)
    {
        $this->goupno = $goupno;

        return $this;
    }

    /**
     * Get goupno
     *
     * @return integer
     */
    public function getGoupno()
    {
        return $this->goupno;
    }

    /**
     * Set sortno
     *
     * @param integer $sortno
     *
     * @return RefFormcontrols
     */
    public function setSortno($sortno)
    {
        $this->sortno = $sortno;

        return $this;
    }

    /**
     * Get sortno
     *
     * @return integer
     */
    public function getSortno()
    {
        return $this->sortno;
    }
}

