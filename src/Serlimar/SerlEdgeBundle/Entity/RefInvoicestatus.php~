<?php

namespace Serlimar\SerlEdgeBundle\Entity;

/**
 * RefInvoicestatus
 */
class RefInvoicestatus
{
    /**
     * @var integer
     */
    private $invoicestatusid;

    /**
     * @var string
     */
    private $status;


    /**
     * Get invoicestatusid
     *
     * @return integer
     */
    public function getInvoicestatusid()
    {
        return $this->invoicestatusid;
    }

    /**
     * Set status
     *
     * @param string $status
     *
     * @return RefInvoicestatus
     */
    public function setStatus($status)
    {
        $this->status = $status;

        return $this;
    }

    /**
     * Get status
     *
     * @return string
     */
    public function getStatus()
    {
        return $this->status;
    }
}

