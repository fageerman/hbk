<?php

namespace Serlimar\SerlEdgeBundle\Entity;

/**
 * Tblmessages
 */
class Tblmessages
{
    /**
     * @var integer
     */
    private $messageid;

    /**
     * @var string
     */
    private $message;

    /**
     * @var \DateTime
     */
    private $msgbegindate;

    /**
     * @var \DateTime
     */
    private $msgenddate;

    /**
     * @var string
     */
    private $msguser;

    /**
     * @var integer
     */
    private $msgcolor;

    /**
     * @var string
     */
    private $postedby;

    /**
     * @var string
     */
    private $memo;

    /**
     * @var \DateTime
     */
    private $timestamp;

    /**
     * @var \DateTime
     */
    private $useractiondate;

    /**
     * @var string
     */
    private $username;

    /**
     * @var string
     */
    private $locked;

    /**
     * @var \DateTime
     */
    private $lockeddate;

    /**
     * @var string
     */
    private $lockeduserid;

    /**
     * @var string
     */
    private $lockedusername;

    /**
     * @var boolean
     */
    private $notactive = '0';


    /**
     * Get messageid
     *
     * @return integer
     */
    public function getMessageid()
    {
        return $this->messageid;
    }

    /**
     * Set message
     *
     * @param string $message
     *
     * @return Tblmessages
     */
    public function setMessage($message)
    {
        $this->message = $message;

        return $this;
    }

    /**
     * Get message
     *
     * @return string
     */
    public function getMessage()
    {
        return $this->message;
    }

    /**
     * Set msgbegindate
     *
     * @param \DateTime $msgbegindate
     *
     * @return Tblmessages
     */
    public function setMsgbegindate($msgbegindate)
    {
        $this->msgbegindate = $msgbegindate;

        return $this;
    }

    /**
     * Get msgbegindate
     *
     * @return \DateTime
     */
    public function getMsgbegindate()
    {
        return $this->msgbegindate;
    }

    /**
     * Set msgenddate
     *
     * @param \DateTime $msgenddate
     *
     * @return Tblmessages
     */
    public function setMsgenddate($msgenddate)
    {
        $this->msgenddate = $msgenddate;

        return $this;
    }

    /**
     * Get msgenddate
     *
     * @return \DateTime
     */
    public function getMsgenddate()
    {
        return $this->msgenddate;
    }

    /**
     * Set msguser
     *
     * @param string $msguser
     *
     * @return Tblmessages
     */
    public function setMsguser($msguser)
    {
        $this->msguser = $msguser;

        return $this;
    }

    /**
     * Get msguser
     *
     * @return string
     */
    public function getMsguser()
    {
        return $this->msguser;
    }

    /**
     * Set msgcolor
     *
     * @param integer $msgcolor
     *
     * @return Tblmessages
     */
    public function setMsgcolor($msgcolor)
    {
        $this->msgcolor = $msgcolor;

        return $this;
    }

    /**
     * Get msgcolor
     *
     * @return integer
     */
    public function getMsgcolor()
    {
        return $this->msgcolor;
    }

    /**
     * Set postedby
     *
     * @param string $postedby
     *
     * @return Tblmessages
     */
    public function setPostedby($postedby)
    {
        $this->postedby = $postedby;

        return $this;
    }

    /**
     * Get postedby
     *
     * @return string
     */
    public function getPostedby()
    {
        return $this->postedby;
    }

    /**
     * Set memo
     *
     * @param string $memo
     *
     * @return Tblmessages
     */
    public function setMemo($memo)
    {
        $this->memo = $memo;

        return $this;
    }

    /**
     * Get memo
     *
     * @return string
     */
    public function getMemo()
    {
        return $this->memo;
    }

    /**
     * Set timestamp
     *
     * @param \DateTime $timestamp
     *
     * @return Tblmessages
     */
    public function setTimestamp($timestamp)
    {
        $this->timestamp = $timestamp;

        return $this;
    }

    /**
     * Get timestamp
     *
     * @return \DateTime
     */
    public function getTimestamp()
    {
        return $this->timestamp;
    }

