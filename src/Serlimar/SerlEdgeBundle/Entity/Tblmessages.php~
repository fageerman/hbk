     * Set lockeduserid
     *
     * @param string $lockeduserid
     *
     * @return Tblmessages
     */
    public function setLockeduserid($lockeduserid)
    {
        $this->lockeduserid = $lockeduserid;

        return $this;
    }

    /**
     * Get lockeduserid
     *
     * @return string
