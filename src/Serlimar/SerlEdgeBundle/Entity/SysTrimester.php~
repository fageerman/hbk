<?php

namespace Serlimar\SerlEdgeBundle\Entity;

/**
 * SysTrimester
 */
class SysTrimester
{
    /**
     * @var integer
     */
    private $trimesterid;

    /**
     * @var integer
     */
    private $trimester;

    /**
     * @var integer
     */
    private $trimestermonthorder;

    /**
     * @var integer
     */
    private $trimestermonthserial;

    /**
     * @var string
     */
    private $trimestermonth;


    /**
     * Get trimesterid
     *
     * @return integer
     */
    public function getTrimesterid()
    {
        return $this->trimesterid;
    }

    /**
     * Set trimester
     *
     * @param integer $trimester
     *
     * @return SysTrimester
     */
    public function setTrimester($trimester)
    {
        $this->trimester = $trimester;

        return $this;
    }

    /**
     * Get trimester
     *
     * @return integer
     */
    public function getTrimester()
    {
        return $this->trimester;
    }

    /**
     * Set trimestermonthorder
     *
     * @param integer $trimestermonthorder
     *
     * @return SysTrimester
     */
    public function setTrimestermonthorder($trimestermonthorder)
    {
        $this->trimestermonthorder = $trimestermonthorder;

        return $this;
    }

    /**
     * Get trimestermonthorder
     *
     * @return integer
     */
    public function getTrimestermonthorder()
    {
        return $this->trimestermonthorder;
    }

    /**
     * Set trimestermonthserial
     *
     * @param integer $trimestermonthserial
     *
     * @return SysTrimester
     */
    public function setTrimestermonthserial($trimestermonthserial)
    {
        $this->trimestermonthserial = $trimestermonthserial;

        return $this;
    }

    /**
     * Get trimestermonthserial
     *
     * @return integer
     */
    public function getTrimestermonthserial()
    {
        return $this->trimestermonthserial;
    }

    /**
     * Set trimestermonth
     *
     * @param string $trimestermonth
     *
     * @return SysTrimester
     */
    public function setTrimestermonth($trimestermonth)
    {
        $this->trimestermonth = $trimestermonth;

        return $this;
    }

    /**
     * Get trimestermonth
     *
     * @return string
     */
    public function getTrimestermonth()
    {
        return $this->trimestermonth;
    }
}

